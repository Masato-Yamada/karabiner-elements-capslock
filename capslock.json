{
  "title": "caps lock",
  "rules": [
    {
      "description": "caps lock with ctrl + shift + caps lock",
      "manipulators": [
        { "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "mandatory": [ "left_shift", "left_control" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ]
        }
      ]
    }
  ]
}
